{
  "version": 2,
  "buildCommand": "cd ../.. && turbo run build --filter={./apps/server}",
  "env": {
    "ENABLE_VC_BUILD": 1
  },
  "builds": [
    {
      "src": "src/main.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/main.ts",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ]
    }
  ]
}